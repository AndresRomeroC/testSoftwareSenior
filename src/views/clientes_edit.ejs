<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.id %>" method="POST">
 
          
          <div class="form-group">
            <input type="text" name="id" value="<%= data.ids%>" placeholder="Ids" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="first_name" value="<%= data.first_name%>" placeholder="Fisrt Name" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="last_name" value="<%= data.last_name%>" placeholder="Last Name" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="email" value="<%= data.email%>" placeholder="Email" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="gender" value="<%= data.gender%>" placeholder="Gender" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="company" value="<%= data.company%>" placeholder="Company" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="city" value="<%= data.city%>" placeholder="City" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="title" value="<%= data.title%>" placeholder="Tittle" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="title" value="<%= data.latitud%>" placeholder="Tittle" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="title" value="<%= data.longuitud%>" placeholder="Tittle" class="form-control">
          </div>
          <div class="form-group">
            <input type="text" name="title" value="<%= data.profileExcelLocation%>" placeholder="Archivo" class="form-control">
          </div>

          <button type="submit" class="btn btn-primary">
            save cliente
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
